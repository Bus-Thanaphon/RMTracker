@model IEnumerable<RMTracker.Core.Models.On_Hold>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CreateAt)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OnholdNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Onhold_Start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Onhold_End)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Onhold_Reason)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CreateAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OnholdNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Onhold_Start)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Onhold_End)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Onhold_Reason)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
